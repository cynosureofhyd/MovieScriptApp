      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="PosterInfoes">
      <EntityTypeMapping TypeName="MovieModel.PosterInfo">
        <MappingFragment StoreEntitySet="PosterInfo">
          <ScalarProperty Name="Id" ColumnName="Id" />
          <ScalarProperty Name="Imdb" ColumnName="Imdb" />
